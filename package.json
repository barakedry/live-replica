{
  "private": true,
  "name": "@live-replica/live-replica",
  "type": "module",
  "version": "0.9.60",
  "description": "real-time objects replica syncing",
  "directories": {
    "test": "tests"
  },
  "scripts": {
    "bootstrap-install": "npm install && lerna bootstrap --no-ci",
    "bootstrap-ci": "npm ci && lerna bootstrap",
    "test": "jest",
    "pack": "npm version patch && npm pack",
    "prepare": "npm run build",
    "build": "lerna run clean && lerna run build --stream --sort",
    "build:watch": "tsc --build --watch",
    "clean": "lerna run clean && rm -rf dist",
    "type-check": "tsc --noEmit",
    "type-check:watch": "tsc --noEmit --watch",
    "dev": "lerna run build:watch --parallel",
    "lint": "lerna run lint --parallel",
    "format": "lerna run format --parallel",
    "version": "lerna version",
    "publish": "lerna publish from-package",
    "changed": "lerna changed",
    "diff": "lerna diff",
    "build:all": "tsc --build packages/utils packages/proxy packages/patch-diff packages/replica packages/ws-client packages/client",
    "clean:all": "tsc --build packages/utils --clean && tsc --build packages/proxy --clean && tsc --build packages/patch-diff --clean && tsc --build packages/replica --clean && tsc --build packages/ws-client --clean && tsc --build packages/client --clean"
  },
  "repository": "git+https://github.com/barakedry/live-replica",
  "keywords": [
    "state",
    "sync",
    "state syncing",
    "reactive",
    "reactive state",
    "real-time",
    "replica",
    "live-replica",
    "patch",
    "merge",
    "diff",
    "delta"
  ],
  "author": "Barak Edry",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/barakedry/live-replica/issues"
  },
  "homepage": "https://github.com/barakedry/live-replica/",
  "devDependencies": {
    "@babel/plugin-transform-modules-commonjs": "^7.23.0",
    "@babel/preset-env": "^7.23.0",
    "@babel/preset-typescript": "^7.23.0",
    "@types/jest": "^29.5.0",
    "@types/lodash-es": "^4.17.12",
    "@types/node": "^20.17.30",
    "@types/ws": "^8.5.0",
    "babel-jest": "^29.7.0",
    "jest": "latest",
    "lerna": "6.6.2",
    "lit": "latest",
    "rimraf": "^5.0.0",
    "typescript": "^5.2.2",
    "ws": "^8.18.0"
  },
  "dependencies": {
    "@msgpack/msgpack": "latest",
    "lodash-es": "latest"
  },
  "main": "index.js",
  "types": "./packages/types/index.d.ts",
  "bundleDependencies": [
    "@msgpack/msgpack",
    "lodash-es"
  ]
}
