{
  "private": true,
  "name": "@live-replica/live-replica",
  "type": "module",
  "version": "0.9.58",
  "description": "real-time objects replica syncing",
  "directories": {
    "test": "tests"
  },
  "scripts": {
    "bootstrap-install": "npm install && lerna bootstrap --no-ci",
    "bootstrap-ci": "npm ci && lerna bootstrap",
    "test": "jest",
    "pack": "npm version patch && npm pack",
    "prepare": "npm run build",
    "build": "lerna run clean && tsc --build packages/events/tsconfig.json packages/utils/tsconfig.json packages/proxy/tsconfig.json packages/patch-diff/tsconfig.json",
    "build:watch": "tsc --build --watch",
    "clean": "lerna run clean && rimraf dist",
    "type-check": "tsc --noEmit",
    "type-check:watch": "tsc --noEmit --watch"
  },
  "repository": "git+https://github.com/barakedry/live-replica",
  "keywords": [
    "state",
    "sync",
    "state syncing",
    "reactive",
    "reactive state",
    "real-time",
    "replica",
    "live-replica",
    "patch",
    "merge",
    "diff",
    "delta"
  ],
  "author": "Barak Edry",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/barakedry/live-replica/issues"
  },
  "homepage": "https://github.com/barakedry/live-replica/",
  "devDependencies": {
    "@babel/plugin-transform-modules-commonjs": "^7.23.0",
    "@types/jest": "^29.5.0",
    "@types/lodash-es": "^4.17.12",
    "@types/node": "^20.17.30",
    "@types/ws": "^8.5.0",
    "babel-jest": "^29.7.0",
    "jest": "latest",
    "lerna": "6.6.2",
    "lit": "latest",
    "rimraf": "^5.0.0",
    "typescript": "^5.2.2",
    "ws": "^8.18.0"
  },
  "dependencies": {
    "@msgpack/msgpack": "latest",
    "lodash-es": "latest"
  },
  "main": "index.js",
  "types": "./packages/types/index.d.ts",
  "bundleDependencies": [
    "@msgpack/msgpack",
    "lodash-es"
  ]
}
